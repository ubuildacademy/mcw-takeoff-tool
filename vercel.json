{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "NODE_ENV=development npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://mcw-takeoff-tool-production.up.railway.app/api/$1"
    }
  ],
  "_comment": "Using Vercel rewrites to proxy API requests to Railway backend. This bypasses Railway Caddy CORS issues."
  "cleanUrls": true,
  "trailingSlash": false
}
















